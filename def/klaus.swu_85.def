/*
* definition of white space model
*/
<space1> <= [space] ;
<space2> <= [space] [space] ;
<space3> <= [space] [space] [space] ;
<space>	:= { <space1> | <space2> | <space3> } ;
/*
 * punctuation symbols
 */
@HYPHEN :=	[-] ;
@COMMA :=	[cm] ;
